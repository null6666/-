page{
  background: #f5f5f5;
}
.nav_left{
  display: inline-block;
  width: 25%;
  height: 100%;
  background: #f5f5f5;
  text-align: center;
}
.nav_left .nav_left_items{
  height: 80rpx;
  line-height: 80rpx;
  padding: 12rpx 0;
  border-bottom: 1px solid #dedede;
  font-size: 28rpx;
}
.nav_left .nav_left_items_active{
  height: 80rpx;
  line-height: 80rpx;
  padding: 12rpx 0;
  border-bottom: 1px solid #dedede;
  font-size: 28rpx;
  background: #fff;
  color: #f0145a;
}
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 75%;
  height: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
}
.nav_right .nav_right_items{
  float: left;
  width: 33.33%;
  height: 240rpx;
  text-align: center;
}
.nav_right .nav_right_items image{
  width: 120rpx;
  height: 120rpx;
  margin-top: 30rpx;
}
.nav_right .nav_right_items text{
  display: block;
  margin-top: 30rpx;
  font-size: 28rpx;
  color: black;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
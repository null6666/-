/* pages/usercenter/index.wxss */
page{
  background-color: #edece8
}
text{
  color: #666;
}
.avatar {
  display: flex;
  width: 56px;
  height: 56px;
}
.containerHead {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
  padding: 150rpx 0;
  box-sizing: border-box;
  background-color: #6dc2fa;
} 
.space{
  width: 100%;
  height: 20rpx;
}
.app_info_wrap {
  margin-top: 15rpx;
  background-color: #fff;
}
.app_info_item {
  padding: 30rpx ;
  border-bottom: 1rpx solid #ccc;
}
.app_info_contact {
  display: flex;
  justify-content: space-between;
}